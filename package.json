    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.19.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "prettier": "3.4.2",
    "typescript": "^5.7.3",
